{
  "name": "react-awesome-boilerplate",
  "version": "1.0.0",
  "description": "A starter template for React with Typescript using Webpack 5",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "analyze": "node ./internals/scripts/analyze.js",
    "security-audit": "npm audit",
    "security-audit:json": "npm audit --json",
    "security-audit:prod": "npm audit --production --only=prod --audit-level=low",
    "security-audit:dev": "npm audit --only=dev --audit-level=moderate",
    "g": "plop --plopfile internals/generators/index.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "commit": "git add . && git-cz",
    "start": "webpack serve --config internals/webpack/webpack.config.js --env env=dev",
    "build-tailwind": "tailwindcss build ./src/styles.css -o ./styles/main.css",
    "build": "webpack --config internals/webpack/webpack.config.js --env env=prod",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint-fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "prettier": "prettier --write",
    "prettify": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "server": "json-server db.json -p 5000 -w -d 0",
    "test": "jest",
    "test:one": "node ./internals/scripts/test.js",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "jest --collect-coverage",
    "open:coverage": "start ./coverage/lcov-report/index.html",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Webpack 5",
    "Starter Template",
    "Boilerplate"
  ],
  "author": "Kaushal",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@sentry/react": "^6.3.6",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.2.6",
    "logatim": "^0.9.0",
    "mobx": "^6.3.0",
    "mobx-react": "^7.1.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-contexify": "^5.0.0",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^1.0.2",
    "react-i18next": "^11.10.0",
    "react-router-dom": "^5.2.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.5",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-twin": "^1.0.2",
    "chalk": "^2.4.2",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.4",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-cz": "^4.7.6",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "plop": "^2.7.4",
    "postcss": "^8.2.15",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^5.3.0",
    "prettier": "^2.3.0",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.2",
    "readline": "^1.3.0",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.5.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "twin.macro": "^2.4.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "emotion"
    }
  }
}
